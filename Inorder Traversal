class Solution {
    public List<Integer> inorderTraversal(TreeNode root) {
        List<Integer> res = new ArrayList();
        order(res,root);
        return res;
    }
 void order(List<Integer> res, TreeNode root){
        if(root==null){
            return;
        }
        
        order(res,root.left);
        res.add(root.val);
        order(res ,root.right);
    
    }
}
